{% extends ":admin:index.html.twig"%}

{% block body %}
      {% block nb %}   
      {% endblock%}
{% block workweb %}
    {% endblock%}
 {% block workprint %}
        {% endblock%}
 {% block workgalerie %}
        {% endblock%}

     <div class="project-content">   
                         {% block ariane %}
             <ol class=" ariane-admin breadcrumb">
  <li><a href="{{path('admin')}}">Home</a></li>
  <li  class="active"> Liste Galerie</li>
</ol>
             {% endblock %}
   <div class="container project">
        <div class="row">
       {% for galerie in projectsgal %}
   
    <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><a class="lien-projet-web" href="{{path("detailprojetsprint",{"id":galerie.id})}}"><i class="icon-add fa fa-desktop"></i>{{galerie.titre}}</a></div>

  <!-- Table -->
  <table class="table">
      <thead> <tr>
              <th>id</th>
              <th>description</th>
              <th>technique</th>
              <th>publier</th>
          </tr>
            <td>{{galerie.id}}</td>
            <td>{{galerie.description|truncate(10, false, ' ...')}}</td>
            <td>{{galerie.technique}}</td>
            {% if galerie.publier ==[1] %}
             <td><i class="fa fa-check"></i></td>
             {% else %}
         <td><i class="fa fa-times"></i></td>
         {% endif %}
         </thead>
  </table>
</div>

 
 {% endfor %}
 </div>
</div>
 
     </div>
{% endblock%}



