{{form_start(formNews,{'attr':{'action':path('update',{'id':id}),'enctype':'multipart/form-data'}})}}

{% else%}
 {{form_start(formNews,{'attr':{'action':path('valid'),'enctype':'multipart/form-data'}})}}

{% endif%}
<div class="row">
<div class="col-sm-6 col-md-6">


    <h1>appercu</h1>
<div class="form-group"><h3>{{form_row(formNews.titre,{'attr':{'class':('form-control')}})}}</h3></div>
<div class="form-group"><h3>{{form_row(formNews.images)}}</h3></div>
