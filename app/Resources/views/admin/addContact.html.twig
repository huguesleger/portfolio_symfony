<body>
         <div class="project-content">
       
       



<div class="bg-form">
         {% block ariane %}
             <ol class=" ariane-admin breadcrumb">
  <li class="txt-color-primary"><a href="{{path('admin')}}">Home</a></li>
  <li class="txt-color-primary"><a href="{{path('contacts')}}">Contact</a></li>
  <li  class="active">Edit</li>
</ol>
             {% endblock %}
<div class="container">
  
     <div class="block-project">
{% if id is defined%}
{{form_start(formContact,{'attr':{'action':path('updatecontact',{'id':id}),'enctype':'multipart/form-data'}})}}

{% else%}
 {{form_start(formContact,{'attr':{'action':path('validcontact'),'enctype':'multipart/form-data'}})}}

{% endif%}
<div class="row">
<div class="col-sm-6 col-md-12 col-md-offset-1">


    <h1 class="title-form"><i class="icon-add-form fa fa-camera-retro"></i>Infos contact</h1>
<div class="input-group col-md-12"><span class="input-group-addon">Email Site</span>{{form_widget(formContact.emailSite,{'attr':{'class':('form-control')}})}}</div>
<div class="input-group col-md-12"><span class="input-group-addon">Lien Site</span>{{form_widget(formContact.lienSite,{'attr':{'class':('form-control')}})}}</div>

   <div class="input-group col-md-12"><span class="input-group-addon">Email Pro</span>{{form_widget(formContact.emailPro,{'attr':{'class':('form-control')}})}}</div>
   <div class="input-group col-md-12"><span class="input-group-addon">Lien Pro</span>{{form_widget(formContact.lienPro,{'attr':{'class':('form-control')}})}}</div>


<div class="input-group col-md-12"><span class="input-group-addon">Code Postal</span>{{form_widget(formContact.codePostal,{'attr':{'class':('form-control')}})}}</div>

<div class="input-group col-md-12"><span class="input-group-addon">Ville</span>{{form_widget(formContact.ville,{'attr':{'class':('form-control')}})}}</div>

</div>



    <div class="col-sm-6 col-md-12 col-md-offset-1">
        

        <div class="input-group right">{{form_widget(formContact.sauvegarder,{'attr':{'class':('btn btn-success ')}})}}</div>
        
    </div>

</div>

        {{form_end(formContact)}}
        

    </div>

</div>
</div>
</div>
        
        
    
             
       
        
        
